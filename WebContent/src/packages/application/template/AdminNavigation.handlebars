{{#if isLoggedIn}}
<div class="btn-group dropdown">
	<button class="btn dropdown-toggle" id="adminNavigationDropdown" data-toggle="dropdown">
		Navigate <span class="caret"></span>
	</button>

	<ul class="dropdown-menu" role="menu" aria-labelledby="adminNavigationDropdown">
		<li><a {{ action gotoPages href=true }}>Pages</a></li>
		<li><a {{ action gotoUsers href=true }}>Users</a></li>
		<li><a href="" {{ action "logout" }}>Logout</a><li>
	</ul>
</div>
{{/if}}